{
    "name": "face-recognition-cdk",
    "version": "1.0.0",
    "description": "AWS CDK Infrastructure for Serverless Face Recognition System",
    "main": "bin/app.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "test": "jest",
        "cdk": "cdk",
        "deploy": "cdk deploy --all --require-approval never",
        "deploy:dev": "cdk deploy FaceRecognitionStack-dev --require-approval never",
        "deploy:prod": "cdk deploy FaceRecognitionStack-prod --require-approval never",
        "destroy": "cdk destroy --all",
        "diff": "cdk diff",
        "synth": "cdk synth",
        "bootstrap": "cdk bootstrap"
    },
    "keywords": [
        "aws",
        "cdk",
        "face-recognition",
        "serverless",
        "sagemaker",
        "iot"
    ],
    "author": "hoangphuc173",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^20.0.0",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.0",
        "aws-cdk": "^2.100.0"
    },
    "dependencies": {
        "aws-cdk-lib": "^2.100.0",
        "constructs": "^10.0.0",
        "dotenv": "^16.0.0"
    }
}
